{
    "name": "sina/shuttle",
    "description": "Smart admin panel for smart programmer",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sina BH",
            "email": "sinabehzadi2014@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "spatie/laravel-permission": "^3.17",
        "cyrildewit/eloquent-viewable": "dev-master",
        "akaunting/laravel-setting": "^1.2",
        "doctrine/dbal": "^2.12",
        "thunderer/shortcode": "^0.7.4",
        "nette/php-generator": "^3.4",
        "astrotomic/laravel-translatable": "^11.9",
        "mcamara/laravel-localization": "^1.6",
        "buglinjo/laravel-webp": "^2.0",
        "pion/laravel-chunk-upload": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Sina\\Shuttle\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sina\\Shuttle\\ShuttleServiceProvider",
                "CyrildeWit\\EloquentViewable\\EloquentViewableServiceProvider::class",
                "Akaunting\\Setting\\Provider::class"
            ],
            "aliases": {
                "Setting": "Akaunting\\Setting\\Facade::class",
                "Shuttle": "Sina\\Shuttle\\Facades\\Shuttle::class"
            }
        }
    }
}
